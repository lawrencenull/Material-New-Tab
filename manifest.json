{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "Material Tab Page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com/; object-src 'self'",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "management",
    "tabs",
    "system.cpu",
    "system.memory"
  ]
}
